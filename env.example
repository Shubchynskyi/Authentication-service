# Database Configuration (for Docker)
DB_URL=jdbc:postgresql://db:5432/auth
DB_USERNAME=postgres
DB_PASSWORD=postgres

# JWT Configuration
JWT_ACCESS_SECRET=your-jwt-access-secret-key-here
JWT_REFRESH_SECRET=your-jwt-refresh-secret-key-here

# Google OAuth2 Configuration
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
# OAuth2 redirect URI (for Docker - without nginx, direct backend access)
OAUTH2_REDIRECT_URI=http://localhost:8080/login/oauth2/code/{registrationId}

# Frontend URL (for Docker)
FRONTEND_URL=http://localhost:3000

# Admin Configuration
ADMIN_ENABLED=true
ADMIN_EMAIL=admin@example.com
ADMIN_USERNAME=admin

# Mail Configuration
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=your-email@gmail.com
MAIL_PASSWORD=your-email-password

